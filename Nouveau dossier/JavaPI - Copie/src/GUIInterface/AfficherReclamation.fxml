<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="345.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController.AfficherReclamationController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="447.0" prefWidth="532.0">
         <children>
            <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="427.0" prefWidth="554.0">
               <children>
                  <TableView fx:id="tableview" layoutX="19.0" layoutY="40.0" onMouseClicked="#clickItem" prefHeight="290.0" prefWidth="517.0">
                     <columns>
                        <TableColumn fx:id="details" prefWidth="213.0" text="Réclamation sur :" />
                        <TableColumn fx:id="date" minWidth="0.0" prefWidth="163.0" text="Date" />
                        <TableColumn fx:id="etat" prefWidth="139.0" text="Etat" />
                     </columns>
                  </TableView>
                  <Button fx:id="ajouter" layoutX="48.0" layoutY="349.0" mnemonicParsing="false" onAction="#ajouterReclamation" style="-fx-background-color: #3CB371;" text="Envoyer une réclamation" />
                  <Button fx:id="supprimer" layoutX="230.0" layoutY="349.0" mnemonicParsing="false" onAction="#supprimerReclamation" style="-fx-background-color: #FF6347;" text="Supprimer une réclamation" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
