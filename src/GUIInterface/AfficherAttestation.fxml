<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController.AfficherAttestationController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="504.0" prefWidth="562.0">
         <children>
            <TableView fx:id="tableview" layoutX="23.0" layoutY="65.0" onMouseClicked="#clickItem" prefHeight="290.0" prefWidth="504.0">
               <columns>
                  <TableColumn fx:id="enfant" prefWidth="179.0" text="Enfant" />
                  <TableColumn fx:id="date" minWidth="0.0" prefWidth="168.0" text="Date" />
                  <TableColumn fx:id="etat" prefWidth="155.0" text="Etat" />
               </columns>
            </TableView>
            <Button fx:id="ajouter" layoutX="45.0" layoutY="376.0" mnemonicParsing="false" onAction="#ajouterAttestation" style="-fx-background-color: #3CB371;" text="Envoyer une attestation" />
            <Button fx:id="supprimer" layoutX="217.0" layoutY="376.0" mnemonicParsing="false" onAction="#supprimerAttestation" style="-fx-background-color: #FF6347;" text="Supprimer l'attestation" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
