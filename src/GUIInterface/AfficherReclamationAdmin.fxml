<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController.AfficherReclamationAdminController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="504.0" prefWidth="850.0">
         <children>
            <TableView fx:id="tableview" layoutX="35.0" layoutY="116.0" onMouseClicked="#clickItem" prefHeight="290.0" prefWidth="796.0">
               <columns>
                  <TableColumn fx:id="parent" prefWidth="112.0" text="ID Parent" />
                  <TableColumn fx:id="details" minWidth="0.0" prefWidth="272.0" text="Réclamation sur" />
                  <TableColumn fx:id="date" prefWidth="235.0" text="Date" />
                  <TableColumn fx:id="etat" prefWidth="191.0" text="Etat" />
               </columns>
            </TableView>
            <Button fx:id="traiter" layoutX="165.0" layoutY="427.0" mnemonicParsing="false" onAction="#traiterReclamation" text="Traiter la réclamation" />
         </children>
      </AnchorPane>
      <Button fx:id="supprimer" layoutX="324.0" layoutY="438.0" mnemonicParsing="false" onAction="#supprimerReclamation" text="Supprimer une réclamation" />
   </children>
</AnchorPane>
