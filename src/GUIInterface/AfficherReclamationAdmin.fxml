<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="452.0" prefWidth="526.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIController.AfficherReclamationAdminController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="442.0" prefWidth="545.0">
         <children>
            <TableView fx:id="tableview" layoutX="38.0" layoutY="36.0" onMouseClicked="#clickItem" prefHeight="290.0" prefWidth="470.0">
               <columns>
                  <TableColumn fx:id="parent" prefWidth="112.0" text="ID Parent" />
                  <TableColumn fx:id="details" minWidth="0.0" prefWidth="101.0" text="Réclamation sur" />
                  <TableColumn fx:id="date" prefWidth="159.0" text="Date" />
                  <TableColumn fx:id="etat" prefWidth="100.0" text="Etat" />
               </columns>
            </TableView>
            <Button fx:id="traiter" layoutX="68.0" layoutY="342.0" mnemonicParsing="false" onAction="#traiterReclamation" style="-fx-background-color: #90EE90;" text="Traiter la réclamation" />
            <Button fx:id="supprimer" layoutX="225.0" layoutY="342.0" mnemonicParsing="false" onAction="#supprimerReclamation" style="-fx-background-color: #FF6347;" text="Supprimer une réclamation" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
