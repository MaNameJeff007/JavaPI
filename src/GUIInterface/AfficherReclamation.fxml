<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="GUIController.AfficherReclamationController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="504.0" prefWidth="850.0">
               <children>
                  <TableView fx:id="tableview" layoutX="8.0" layoutY="118.0" onMouseClicked="#clickItem" prefHeight="290.0" prefWidth="796.0">
                     <columns>
                        <TableColumn fx:id="date" prefWidth="213.0" text="Date" />
                        <TableColumn fx:id="etat" minWidth="0.0" prefWidth="190.0" text="Etat" />
                        <TableColumn fx:id="note" prefWidth="192.0" text="Note" />
                        <TableColumn fx:id="parent" prefWidth="200.0" text="Parent" />
                     </columns>
                  </TableView>
                  <Button fx:id="ajouter" layoutX="132.0" layoutY="428.0" mnemonicParsing="false" onAction="#ajouterReclamation" text="Envoyer une réclamation" />
                  <Button fx:id="supprimer" layoutX="314.0" layoutY="428.0" mnemonicParsing="false" onAction="#supprimerReclamation" text="Supprimer une réclamation" />
                  <Button layoutX="27.0" layoutY="24.0" mnemonicParsing="false" onAction="#retour" text="Retour à l'accueil" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
